#include <stdio.h>
#include <string.h>

int main() {
    char username1[20], username2[20];

    printf("Enter the first username: ");
    scanf("%s", username1);

    printf("Enter the second username: ");
    scanf("%s", username2);

    if (strcmp(username1, username2) ==0 ) {
        printf("The two usernames are valid");
    }
    else {
        printf("The two usernames are invalid");
    }

    return 0;
}
